<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="jcw">
	<Appenders>
		<Console name="stdout" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{yyyy-mm-dd HH:mm:ss} %p %m%n" />
		</Console>
	</Appenders>
	
	<Loggers>
		<Root level="debug">
			<AppenderRef ref="stdout" />
		</Root>
	</Loggers>
</Configuration>